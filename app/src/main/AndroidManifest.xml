<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.johnny.wallhack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/raven_head"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActualGame" />
        <activity android:name=".GetName" />
        <activity android:name=".Help_Page"/>
        <activity android:name=".Leaderboard" />
        <activity android:name=".End"></activity>
    </application>

</manifest>